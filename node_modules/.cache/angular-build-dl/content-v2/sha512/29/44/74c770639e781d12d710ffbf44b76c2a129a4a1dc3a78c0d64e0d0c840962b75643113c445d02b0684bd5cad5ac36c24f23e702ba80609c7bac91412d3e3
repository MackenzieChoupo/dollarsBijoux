g = true;\n    this.loading.show();\n    this.http.get(this.config.url + '/api/appusers/ionic_reward_points/?insecure=cool&user_id=' + this.shared.customerData.id).subscribe((data: any) => {\n      this.httpLoading = false;\n      this.loading.hide();\n      let dat = data.data;\n      this.rewards = dat;\n      console.log(dat);\n    });\n  }\n\n  openShop() {\n    this.navCtrl.navigateForward(\"/products/0/0/newest\");\n  }\n  ionViewDidLoad() {\n    console.log('ionViewDidLoad RewardPointsPage');\n  }\n  refreshPage() {\n    this.getRewards();\n  }\n  totalPoints() {\n    let t = 0;\n    for (let v of this.rewards) {\n      t = t + parseInt(v.points);\n    }\n    return t;\n  }\n\n\n  ngOnInit() {\n  }\n\n}\n"],"sourceRoot":"webpack:///"}